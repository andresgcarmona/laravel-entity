{
  "name": "polaris/laravel-entity",
  "description": "A Laravel package to give your PHP standard classes identity and extra functionality on the fly.",
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "orchestra/testbench": "^7.0@dev"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Andres G. Carmona",
      "email": "ing.andres.carmona@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "^7.3",
    "ext-json": "*",
    "illuminate/support": "^9.0@dev"
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "autoload": {
    "psr-4": {
      "Polaris\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Polaris\\LaravelEntityServiceProvider"
      ]
    }
  }
}
